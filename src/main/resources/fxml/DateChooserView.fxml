<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.HBox?>

<fx:root fx:id="view" alignment="CENTER" spacing="5.0" type="HBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ComboBox fx:id="diaChooser" prefWidth="75.0" />
      <ComboBox fx:id="mesChooser" prefWidth="125.0" />
      <ComboBox fx:id="yearChooser" editable="true" prefWidth="125.0" />
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</fx:root>
